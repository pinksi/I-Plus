            android:label="@string/title_activity_va_result" >
        </activity>
        <activity
            android:name=".cbresult"
            android:label="@string/title_activity_cbresult" >
        </activity>

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="AIzaSyDOMXqmkOSZAa-NT6PqXf2LN5xfsF7Vnls" />

        <activity
