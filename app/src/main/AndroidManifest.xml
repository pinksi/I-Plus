        <activity
            android:name=".ColorBlindness"
            android:label="@string/title_activity_color_blindness" >
        </activity>
        <activity
            android:name=".Astigmatism"
            android:label="@string/title_activity_astigmatism" >
        </activity>
        <activity
            android:name=".Astigma"
            android:label="@string/title_activity_astigma" >
